{
    "testFiles": "**/*.{feature,features}",
    "baseUrl":"https://www.demoblaze.com/index.html",
  "ignoreTestFiles": ["*.js", "*.md", "*.ts"],
    "reporter": "./node_modules/mochawesome/src/mochawesome.js",
